<?xml version="1.0"?>
<odoo>
  <record id="view_form_staff" model="ir.ui.view">
    <field name="name">Staff Member Form View</field>
    <field name="model">sis.lecturer</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="name" />
          <field name="surname" />
          <field name="email" />
          <field name="password"/>
          <field name="id" />
          <field name="department" />
          <field name="level"/>
        </group>
      </form>
    </field>
  </record>

  <record id="view_tree_staff" model="ir.ui.view">
    <field name="name">Staff Member List View</field>
    <field name="model">sis.lecturer</field>
    <field name="arch" type="xml">
      <tree>
          <field name="name" />
          <field name="surname" />
          <field name="email" />
      </tree>
    </field>
  </record>
</odoo>
