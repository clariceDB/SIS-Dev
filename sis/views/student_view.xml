<?xml version="1.0"?>
<odoo>
  <record id="view_form_student" model="ir.ui.view">
    <field name="name">Student Form View</field>
    <field name="model">sis.student</field>
    <field name="arch" type="xml">
      <form>
        <group>
			<field name="name" />
			<field name="surname" />
			<field name="email" />
			<field name="password"/>
			<field name="id" />
			<field name="programme" />
			<field name="current_year"/>
			<field name="programme" />
			<field name="address" />
			<field name="phone" />
			<field name="school" />
        </group>
      </form>
    </field>
  </record>

  <record id="view_tree_student" model="ir.ui.view">
    <field name="name">SStudent List View</field>
    <field name="model">sis.student</field>
    <field name="arch" type="xml">
      <tree>
          <field name="name" />
          <field name="surname" />
          <field name="email" />
		  <field name="programme" />
		  <field name="current_year" />
      </tree>
    </field>
  </record>
</odoo>